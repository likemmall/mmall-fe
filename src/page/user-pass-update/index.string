    <div class="error-item">
            <i class="fa fa-minus-circle error-icon"></i>
            <p class="err-message">error</p>
        </div>
    <div class="form-line">
        <a href="javascript:;" class="u-btn u-btn-mini" id="btn-submit">提交修改</a>
    </div>
</div>